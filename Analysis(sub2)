    {
      "cell_type": "code",
      "source": [
        " from scipy.stats import ttest_ind\n",
        "\n",
        "def welch_ttest(df, col):\n",
        "    pre = df[df[\"period\"]==\"Pre-Sport Republic\"][col].dropna()\n",
        "    post = df[df[\"period\"]==\"Post-Sport Republic\"][col].dropna()\n",
        "    t, p = ttest_ind(pre, post, equal_var=False)\n",
        "    return len(pre), len(post), t, p, pre.mean(), post.mean()\n",
        "\n",
        "results = []\n",
        "results.append((\"90s\",) + welch_ttest(misc_df, \"90s\"))\n",
        "results.append((\"G+A per 90\",) + welch_ttest(standard_df, \"G+A per 90\"))\n",
        "results.append((\"def_actions\",) + welch_ttest(misc_df, \"def_actions\"))\n",
        "\n",
        "out = pd.DataFrame(results, columns=[\"metric\",\"n_pre\",\"n_post\",\"t_stat\",\"p_value\",\"mean_pre\",\"mean_post\"])\n",
        "out"
      ],
