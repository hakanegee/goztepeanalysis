        "plt.title(\"Goal contributions per 90 — Pre vs Post\")\n",
        "plt.show()\n",
        "\n",
        "# Defensive actions: Interceptions + Tackles Won\n",
        "misc_df[\"def_actions\"] = misc_df[\"Interceptions\"].fillna(0) + misc_df[\"Tackles Won\"].fillna(0)\n",
        "\n",
        "plt.figure(figsize=(7,4))\n",
        "sns.boxplot(data=misc_df, x=\"period\", y=\"def_actions\")\n",
        "plt.title(\"Defensive actions (Int + TklW) — Pre vs Post\")\n",
        "plt.show()"
